{
  "compilerOptions": {
    "target": "es5", // You can also set this to 'esnext'
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "module": "esnext",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "next-env.d.ts",
    ".next/types/**/*.ts"
, "app/layout.jsx", "app/page.jsx", "app/pages/directions.jsx", "app/pages/about.jsx", "app/pages/hero.jsx", "app/pages/reviews.jsx", "app/pages/priceMenu.jsx", "app/pages/instagram.jsx", "next.config.js"  ],
  "exclude": [
    "node_modules"
  ]
}
